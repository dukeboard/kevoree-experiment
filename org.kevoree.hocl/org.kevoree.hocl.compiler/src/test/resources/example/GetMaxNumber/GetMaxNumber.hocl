let removeElement = 
	replace x::Object, "REMOVE":<y::Object, ?w>
	by "REMOVE":<w>
	if (x.equals(y))
in

let passInsertTuple =
	replace x::String:<?w>, "INSERT":x::String:<?l>
	by x:<w,l>
in

let passRemoveTuple =
	replace x::String:<?w>, "REMOVE":x::String:<?l>
	by x:<w,l>
in

let removeTrashTuple =
	replace x::String:<"REMOVE":<?y>, ?l>
	by x:<l>
in

let getMaxNumber=
	replace x::int, y::int
	by x
	if x>=y
in

